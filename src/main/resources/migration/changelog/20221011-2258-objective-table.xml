<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

  <changeSet author="John" id="someUniqueId">
    <createTable tableName="objective">
      <column name="objective_id" type="bigint" autoIncrement="true">
        <constraints primaryKey="true"/>
      </column>
      <column name="title" type="text">
        <constraints unique="true" uniqueConstraintName="unique_objective_title" nullable="false"/>
      </column>
      <column name="type" type="varchar(255)"/>
      <column name="mentor" type="text">
        <constraints nullable="false"/>
      </column>
      <column name="reason" type="text">
        <constraints nullable="false"/>
      </column>
      <column name="start_date" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="due_date" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="created_at" type="bigint"/>
      <column name="updated_at" type="bigint"/>
    </createTable>
  </changeSet>

</databaseChangeLog>